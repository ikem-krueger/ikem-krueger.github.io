<policyconfig>

	<vendor>Virtual Machine Manager</vendor>
	<vendor_url>https://virt-manager.org/</vendor_url>

	<action id="org.virt-manager">
		<icon_name>virt-manager</icon_name>
		<message>Virtual Machine Manager</message>
		<message xml:lang="as">ভাৰ্চুৱেল ডিভাইচ ব্যৱস্থাপক</message>
		<message xml:lang="bg">Мениджър на виртуални машини</message>
		<message xml:lang="bn_IN">ভার্চুয়াল মেশিন ম্যানেজার</message>
		<message xml:lang="bs">Upravljač virtualnog računala</message>
		<message xml:lang="ca">Eina de gestió de màquines virtuals</message>
		<message xml:lang="cs">Správce virtuálních strojů</message>
		<message xml:lang="da">Administration for virtuel maskine</message>
		<message xml:lang="de">Virtuelle Maschinenverwaltung</message>
		<message xml:lang="en_GB">Virtual Machine Manager</message>
		<message xml:lang="es">Gestor de máquinas virtuales</message>
		<message xml:lang="fi">Virtuaalikoneiden hallitsin</message>
