		<message xml:lang="pa">ਵਰਚੁਅਲ ਮਸ਼ੀਨ ਮੈਨੇਜਰ</message>
		<message xml:lang="pl">Menedżer maszyn wirtualnych</message>
		<message xml:lang="pt_BR">Gerenciador de máquinas virtuais</message>
		<message xml:lang="pt">Gestor de Máquinas Virtuais</message>
